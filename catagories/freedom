<script>
$( function(){
    $.ajax({
        url: 'https://psd.sector.blue/data.txt',
        type: 'get',
        success: function(data) {
            if (data.substr(0,1) == 0)
                {$("#data").html('<div style="color:red; border:2px dotted blue;">Alpha</div>');}
            if (data.substr(0,1) == 1)
                {$("#data").html('<div>Beta</div>');}
        },
    });
});



